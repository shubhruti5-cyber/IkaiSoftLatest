                                <div class="form-input-box">
                                    <div class="form-input-title">
                                        <label for="program detail">Program Description<span>*</span></label>
                                    </div>
                                    <div class="form-input">
                                        <textarea id="program detail" rows="2" placeholder="Enter program details want to conduct "></textarea>
                                    </div>
                                </div>
                            </div>
                            <div class="d-flex-between mt-30">
                                <button class="prevBtn bd-btn btn-secondary" type="button">Previous</button>
                                <button class="nextBtn bd-btn btn-primary" type="button">Submit</button>
                            </div>
                        </div>

